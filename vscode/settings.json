
{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.fontWeight": "700",
  "editor.fontFamily": "SF Mono",
  "editor.fontSize": 11,
  "terminal.integrated.fontWeight": "600",
  "terminal.integrated.fontSize": 11,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDelete": false,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.arrowParens": "avoid",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      "wrap_attributes": "force-expand-multiline"
    },
    "prettyhtml": {
      "printWidth": 100,
      "singleQuote": false,
      "wrapAttributes": false,
      "sortAttributes": false
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "gruvboxMaterial.diagnosticTextBackgroundOpacity": "50%",
  "gruvboxMaterial.lightWorkbench": "flat",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.parameterHints.enabled": false,
  "explorer.compactFolders": false,
  "eslint.format.enable": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.eslintIntegration": true,
  "editor.lineHeight": 16,
  "editor.suggestLineHeight": 20,
  "editor.renderLineHighlight": "gutter",
  "gruvboxMaterial.italicComments": false,
  "editor.cursorWidth": 3,
  "editor.letterSpacing": 0.1,
  "workbench.fontAliasing": "antialiased",
  "diffEditor.wordWrap": "on",
  "editor.wordWrap": "on",
  "editor.suggestSelection": "first",
  "gruvboxMaterial.colorfulSyntax": true,
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".css",
      "savePath": "~/../css/"
    }
  ],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.activityBar.visible": false,
  "editor.minimap.enabled": false,
  "tabnine.experimentalAutoImports": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontLigatures": false,
  "gruvboxMaterial.darkWorkbench": "high-contrast",
  "workbench.sideBar.location": "right",
  "workbench.iconTheme": "material-icon-theme",
  "prettier.printWidth": 120,
  "vim.cursorStylePerMode.insert": "line",
  "editor.tabSize": 2,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-p>": false
  },
  "workbench.colorTheme": "Minimal Theme",
  "window.zoomLevel": -1,
  "liveServer.settings.donotVerifyTags": true
}
